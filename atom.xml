---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>OAccounts</title>
    <link href="http://feeds2.feedburner.com/oaccounts" rel="self"/>
    <link href="http://ept.github.com/oaccounts/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://ept.github.com/oaccounts/</id>
    <author>
        <name>Martin Kleppmann</name>
        <email>martin@eptcomputing.com</email>
    </author>

    {% for post in site.posts %}
        <entry>
            <title>{{ post.title }}</title>
            <link href="http://ept.github.com/oaccounts{{ post.url }}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <id>http://ept.github.com/oaccounts{{ post.id }}</id>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}
</feed>
